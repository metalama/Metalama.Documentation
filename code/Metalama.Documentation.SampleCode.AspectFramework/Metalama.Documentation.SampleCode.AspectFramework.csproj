<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Library</OutputType>
        <TargetFramework>net6.0</TargetFramework>
        <StartupObject>AutoGeneratedProgram</StartupObject>
        <RootNamespace>Doc</RootNamespace>
        <LangVersion>9.0</LangVersion>
        <NoWarn>1701;1702;169;649;67;8618;8618</NoWarn>
    </PropertyGroup>

    <ItemGroup>
      <Compile Remove="AddEditorBrowsableAttribute - Copy.t.cs" />
      <Compile Remove="Architecture\Experimental - Copy.t.cs" />
    </ItemGroup>

    <ItemGroup>
      <None Include="AddEditorBrowsableAttribute_Introduced_Declarative.t.cs">
        <SubType>Code</SubType>
        <DependentUpon>AddEditorBrowsableAttribute_Introduced_Declarative.cs</DependentUpon>
      </None>
      <None Include="Architecture\Experimental_Fabric.t.cs">
        <SubType>Code</SubType>
        <DependentUpon>Experimental_Fabric.cs</DependentUpon>
      </None>
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.NET.Test.Sdk" Version="16.7.1" />
        <PackageReference Include="Microsoft.Win32.Registry" Version="5.0.0" />
        <PackageReference Include="xunit" Version="2.4.1" />
        <PackageReference Include="xunit.runner.visualstudio" Version="2.4.3">
            <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
            <PrivateAssets>all</PrivateAssets>
        </PackageReference>

        <PackageReference Include="Metalama.Extensions.Architecture" Version="$(MetalamaExtensionsVersion)" />
        <PackageReference Include="Metalama.Testing.AspectTesting" Version="$(MetalamaVersion)" />
    </ItemGroup>

    <ItemGroup>
      <Compile Update="AddEditorBrowsableAttribute_Introduced_Programmatic.Aspect.cs">
        <DependentUpon>$([System.String]::Copy('%(Filename)').Replace('.Aspect','')).cs</DependentUpon>
      </Compile>
    </ItemGroup>


  
</Project>
