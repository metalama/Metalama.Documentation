<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Library</OutputType>
		<TargetFramework>net7.0-windows</TargetFramework>
		<StartupObject>AutoGeneratedProgram</StartupObject>
		<RootNamespace>Doc</RootNamespace>
		<NoWarn>1701;1702;169;649;67;8618;8618</NoWarn>
		<UseWPF>true</UseWPF>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.NET.Test.Sdk" />
		<PackageReference Include="Microsoft.Win32.Registry" />
		<PackageReference Include="xunit" />
		<PackageReference Include="xunit.runner.visualstudio">
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
			<PrivateAssets>all</PrivateAssets>
		</PackageReference>

		<PackageReference Include="Metalama.Patterns.Wpf" />
		<PackageReference Include="Metalama.Patterns.Observability" />
		<PackageReference Include="Metalama.Testing.AspectTesting" />
	</ItemGroup>

</Project>
