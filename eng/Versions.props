
<Project>

    <!-- Set the default version -->
    <Import Project="MainVersion.props" />
    
    <PropertyGroup>
        <MetalamaDocumentationVersion>$(MainVersion)$(PackageVersionSuffix)</MetalamaDocumentationVersion>
        <MetalamaDocumentationAssemblyVersion>$(MainVersion)</MetalamaDocumentationAssemblyVersion>
    </PropertyGroup>

    <!-- Versions of dependencies -->
    <PropertyGroup>
        <PostSharpEngineeringVersion>2023.1.13-preview</PostSharpEngineeringVersion>
        <MetalamaVersion>2023.1.0-preview</MetalamaVersion>
        <MetalamaCompilerVersion>2023.1.0-preview</MetalamaCompilerVersion>
        <MetalamaMigrationVersion>2023.1.0-preview</MetalamaMigrationVersion>
        <MetalamaExtensionsVersion>2023.1.0-preview</MetalamaExtensionsVersion>
        <MetalamaLinqPadVersion>2023.1.0-preview</MetalamaLinqPadVersion>
        <MetalamaSamplesVersion></MetalamaSamplesVersion>
        <NuGetCommandLineVersion>6.4.0</NuGetCommandLineVersion>
    </PropertyGroup>

    <!-- Import overrides for the local build -->
    <Import Project="Versions.g.props" Condition="Exists('Versions.g.props')" />

    <!-- Other properties dependent on the versions set above. -->
    <PropertyGroup>
        <AssemblyVersion>$(MetalamaDocumentationAssemblyVersion)</AssemblyVersion>
        <Version>$(MetalamaDocumentationVersion)</Version>
    </PropertyGroup>

</Project>