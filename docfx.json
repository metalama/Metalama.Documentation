{
  "metadata": [
    {
      "src": [
        {
          "files": [
              "Metalama.Framework.dll",
              "Metalama.Framework.RunTime.dll",
              "Metalama.Testing.AspectTesting.dll",
              "Metalama.Testing.UnitTesting.dll",
              "Metalama.Framework.Sdk.dll",
              "Metalama.Framework.Introspection.dll",
              "Metalama.Framework.Workspaces.dll",
              // Intentionally ignoring Metalama.LinqPad because it has an extension method for `object`.
              // "Metalama.LinqPad.dll",
              "Metalama.Compiler.Interface.dll",
              "Metalama.Migration.dll",
              "Metalama.Extensions.DependencyInjection.dll",
              "Metalama.Extensions.DependencyInjection.ServiceLocator.dll",
              "Metalama.Extensions.Multicast.dll",
              "Metalama.Extensions.Metrics.dll",
              "Metalama.Extensions.Architecture.dll",
              "Metalama.Patterns.Contracts.dll",
              "Metalama.Patterns.Memoization.dll",
              "Metalama.Patterns.Immutability.dll",
              "Metalama.Patterns.Observability.dll",
              "Metalama.Patterns.Wpf.dll",
              "Metalama.Patterns.Caching.dll",
              "Metalama.Patterns.Caching.Backend.dll",
              "Metalama.Patterns.Caching.Aspects.dll",
              "Metalama.Patterns.Caching.Backends.Azure.dll",
              "Metalama.Patterns.Caching.Backends.Redis.dll",
              "Flashtrace.dll",
              "Flashtrace.Formatters.dll"
              ],
          "src": "code/Metalama.Documentation.Consolidated/bin/Debug/net8.0-windows/all"
        }
      ],
      "dest": "artifacts/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "memberLayout": "separatePages"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ],
        "src": "artifacts"
      },
      {
        "files": [
          "**.md",
          "toc.yml"
        ],
        "src": "content"
      },
      {
        "files": [
          "examples/**.md"
        ],
        "src": "source-dependencies/Metalama.Samples"
      }
    ],
    "resource": [
      {
        "files": [
          "**.png",
          "**.svg",
          "api.xml",
          "redirect.xml"
        ],
        "src": "content"
      },
      {
        "files": [
          "**.png"
        ],
        "src": "source-dependencies/Metalama.Samples"
      }
    ],
    "overwrite": [
      {
        "files": [
          "overwrite/**.md"
        ]
      }
    ],
    "xref": [
      "https://learn.microsoft.com/en-us/dotnet/.xrefmap.json"
    ],
    "dest": "artifacts/site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template":[ 
      "default", 
      "templates"
    ],
    // Used for debugging
    // "maxParallelism": 1,
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false
  }
}